{
  "name": "assignment-approval-system",
  "version": "1.0.0",
  "description": "Full-stack Assignment Approval System with React frontend and Express backend",
  "scripts": {
    "install:all": "npm run install:backend && npm run install:client",
    "install:backend": "cd backend && npm install",
    "install:client": "cd client && npm install",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:client\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:client": "cd client && npm run dev",
    "start:backend": "cd backend && npm start",
    "build:client": "cd client && npm run build"
  },
  "keywords": ["assignment", "approval", "react", "express", "mongodb"],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
